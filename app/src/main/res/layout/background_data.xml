<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_transparent">


    <ImageView
        android:id="@+id/iv_leftstart"
        android:layout_width="136dp"
        android:layout_height="169dp"
        android:contentDescription="@null"
        android:src="@drawable/left_upcorner" />


    <ImageView
        android:id="@+id/iv_rgtend"
        android:layout_width="88dp"
        android:layout_height="121dp"
        android:layout_alignParentEnd="true"
        android:contentDescription="@null"
        app:srcCompat="@drawable/right_upcorner" />

    <ImageView
        android:id="@+id/iv_leftstartbotm"
        android:layout_width="98dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/leftcorner"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:contentDescription="@null"/>

    <ImageView
        android:id="@+id/iv_rgtendbotm"
        android:layout_width="144dp"
        android:layout_height="162dp"
        app:srcCompat="@drawable/rightcorner"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:contentDescription="@null"/>



</RelativeLayout>